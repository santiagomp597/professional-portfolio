---
import ExperienceItem from "./ExperienceItem.astro";
import type { ExperienceItemI } from "@/interfaces/translation";

interface Props {
  experienceItems: ExperienceItemI[];
}

const { experienceItems } = Astro.props;
---

<ol class="relative mt-5">
  {
    experienceItems.map((experienceItem) => (
      <li>
        <ExperienceItem {...experienceItem} />
      </li>
    ))
  }
</ol>
